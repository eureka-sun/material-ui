// https://raw.githubusercontent.com/MSLacerda/brisanet-web/develop/tsconfig.json?token=AIITYHLWDZI55L6NPYT3X6K6JHTAC

{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "allowJs": false,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "lib": [
      "dom",
      "es2017"
    ],
    "paths": {
      "@app/*": [
        "./src/*"
      ],
    },
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "exclude": [
    "node_modules",
  ]
}
